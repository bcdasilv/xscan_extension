<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="Aspect_Mining.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               class="aspect_mining.actions.CBFAAction"
               icon="icons/sample.gif"
               id="aspect_mining.actions.CBFAAction"
               label="Run CBFA"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup">
         </action>
         <action
               label="Run AspectWiz"
               icon="icons/sample.gif"
               class="aspect_mining.actions.AspectWizAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="aspect_mining.actions.AspectWizAction">
         </action>
      </actionSet>
   </extension>

</plugin>
